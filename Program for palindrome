#include<stdio.h>
 
int main()
{
    int n, last_digit,rev=0,num;
    print("enter any number");
    scanf("%d",&n);
    temp=n;
    
    while(n!=0)
    {
        last_digit=n%10;
        rev=last_digit+rev*10;
        n=n/10;
    }
    
    if(temp==n)
        printf("\nThe given number is palindrome");
    else
        printf("\nThe given number is not palindrome");
 
    return 0;
}
